{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "plugs:list": "umi plugin list",
    "plugs:list:k": "umi plugin list --key",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "serve": "set PORT=3000 && umi-serve",
    "linux:serve": "PORT=3000 && umi-serve",
    "pm2" : "pm2 start npm --name 'Frontend App' -- run linux:serve",
    "b:serve" : "npm run build && npm run serve",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
